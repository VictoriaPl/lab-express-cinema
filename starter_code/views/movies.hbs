<h1>Ironhack Cinema</h1>
 <p>Click on the movie to check the showtimes</p>
<div class="row">
  <div class="col-md-3 d-inline">
    {{#each data}}
      <img src={{this.image}} alt="imgMovie"/>
      <p><strong>{{this.title}}</strong></p>
      <a href="/movies/{{this._id}}">See more</a><hr>
    {{/each}}
  </div>  
</div>
